{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "aws_access_key": "MyAwsAccessKey",
  "aws_secret_key": "MyAwsSecretKey",
  "aws_bucket_name": "MyAwsBucketName",
  // renew token from https://api.slack.com/authentication/config-tokens, click generate token
  "slack_config_access_token": "MySlackConfigAccessToken",
  "slack_config_refresh_token": "MySlackConfigRefreshToken"
}
